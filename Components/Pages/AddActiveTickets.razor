@page "/add-active-tickets"@attribute [Authorize]<PageTitle>Add TblTicket</PageTitle><RadzenColumn SizeMD=12>    <RadzenAlert Shade="Shade.Lighter" Variant="Variant.Flat" Size="AlertSize.Small" AlertStyle="AlertStyle.Danger" Visible="@errorVisible">Cannot save TblTicket</RadzenAlert>    <RadzenTemplateForm TItem="RhinoTicketingSystem.Models.db_a79800_ticket.TblTicket" Data="@tblTicket" Visible="@(tblTicket != null)" Submit="@FormSubmit">        <RadzenStack>            <RadzenFormField Text="Ticket Number" Variant="Variant.Filled">                <ChildContent>                    <RadzenTextBox style="display: block; width: 100%" @bind-Value="@tblTicket.TicketNumber" Name="TicketNumber" />                </ChildContent>            </RadzenFormField>            <RadzenFormField Text="Ticket Header" Variant="Variant.Filled">                <ChildContent>                    <RadzenTextBox style="display: block; width: 100%" @bind-Value="@tblTicket.TicketHeader" Name="TicketHeader" />                </ChildContent>            </RadzenFormField>            <RadzenFormField Text="Ticket Description" Variant="Variant.Filled">                <ChildContent>                    <RadzenTextBox style="display: block; width: 100%" @bind-Value="@tblTicket.TicketDescription" Name="TicketDescription" />                </ChildContent>            </RadzenFormField>            <RadzenFormField Text="Eng Email" Variant="Variant.Filled">                <ChildContent>                    <RadzenTextBox style="display: block; width: 100%" @bind-Value="@tblTicket.EngEmail" Name="EngEmail" />                </ChildContent>            </RadzenFormField>            <RadzenFormField Text="Attachment" Variant="Variant.Filled">                <ChildContent>                    <RadzenTextBox style="display: block; width: 100%" @bind-Value="@tblTicket.Attachment" Name="Attachment" />                </ChildContent>            </RadzenFormField>            <RadzenFormField Text="Attched File Name" Variant="Variant.Filled">                <ChildContent>                    <RadzenTextBox style="display: block; width: 100%" @bind-Value="@tblTicket.AttchedFileName" Name="AttchedFileName" />                </ChildContent>            </RadzenFormField>            <RadzenFormField Text="Attched File Size" Variant="Variant.Filled">                <ChildContent>                    <RadzenNumeric style="display: block; width: 100%" @bind-Value="@tblTicket.AttchedFileSize" Name="AttchedFileSize" />                </ChildContent>            </RadzenFormField>            <RadzenFormField Text="Engineer Comment" Variant="Variant.Filled">                <ChildContent>                    <RadzenTextBox style="display: block; width: 100%" @bind-Value="@tblTicket.EngineerComment" Name="EngineerComment" />                </ChildContent>            </RadzenFormField>            <RadzenFormField Text="Ticket Status" Variant="Variant.Filled">                <ChildContent>                    <RadzenDropDownDataGrid Data="@tblStatusesForStatusId" TextProperty="Description" ValueProperty="StatusId" AllowClear=true                         style="display: block; width: 100%" @bind-Value="@(tblTicket.StatusId)" Name="TicketStatus" />                </ChildContent>            </RadzenFormField>                        <RadzenFormField Text="Date" Variant="Variant.Filled">                <ChildContent>                    <RadzenDatePicker DateFormat="MM/dd/yyyy" style="display: block; width: 100%" @bind-Value="@tblTicket.Date" Name="Date" />                </ChildContent>            </RadzenFormField>            <RadzenFormField Text="Tbl Category" Variant="Variant.Filled">                <ChildContent>                    <RadzenDropDownDataGrid Data="@tblCategoriesForCategoryId" TextProperty="Description" ValueProperty="CategoryId" AllowClear=true                                            style="display: block; width: 100%" @bind-Value="@tblTicket.CategoryId" Name="CategoryId" />                </ChildContent>            </RadzenFormField>            <RadzenFormField Text="Tbl Engineer" Variant="Variant.Filled">                <ChildContent>                    <RadzenDropDownDataGrid Data="@tblEngineersForEngineerId" TextProperty="Name" ValueProperty="Id" AllowClear=true                                            style="display: block; width: 100%" @bind-Value="@tblTicket.EngineerId" Name="EngineerId" />                </ChildContent>            </RadzenFormField>            <RadzenFormField Text="User Email" Variant="Variant.Filled">                <ChildContent>                    <RadzenTextBox style="display: block; width: 100%" @bind-Value="@tblTicket.UserEmail" Name="UserEmail" />                </ChildContent>            </RadzenFormField>        </RadzenStack>        <RadzenStack style="margin-top:1rem;" Orientation="Orientation.Horizontal" AlignItems="AlignItems.Center" JustifyContent="JustifyContent.End" Gap="0.5rem">            <RadzenButton ButtonStyle="ButtonStyle.Primary" ButtonType="ButtonType.Submit" Icon="save" Text="Save" Variant="Variant.Flat" />            <RadzenButton ButtonStyle="ButtonStyle.Light" Text="Cancel" Variant="Variant.Flat" Click="@CancelButtonClick" />        </RadzenStack>    </RadzenTemplateForm></RadzenColumn>